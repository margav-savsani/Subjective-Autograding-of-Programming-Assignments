withchoose:
	g++ -c journey.cpp -o journey.o
	g++ -c tree.cpp -o tree.o
	g++ -c choose.cpp -o choose.o
	g++ -c sort.cpp -o sort.o
	g++ -c makeTree.cpp -o makeTree.o
	g++ -o sortedTree makeTree.o journey.o tree.o sort.o choose.o
without:
	g++ -c journey.cpp -o journey.o
	g++ -c tree.cpp -o tree.o
	g++ -c sort.cpp -o sort.o
	g++ -c makeTree.cpp -o makeTree.o
	g++ -o sortedTree makeTree.o journey.o tree.o sort.o choose.o
